function dataset = raw_data_parser(data_stream)

dataset = struct('odom', {}, 'laser', {});

laser_i = 1;
odom_i = 1;

while ~feof(data_stream)
    unit_measurement = strplit(fgets(data_stream));
    if unit_measurement(4) == 'position'
        dataset.odom(odom_i) = odom_parser(unit_measurement);
    elseif unit_measurement(4) == 'laser'
        dataset.laser(laser_i) = laser_parser(unit_measurement);
    end
end

dataset